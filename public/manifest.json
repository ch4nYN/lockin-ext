{
  "manifest_version": 3,
  "name": "Lockin.",
  "version": "1.0",
  "permissions": ["storage", "alarms", "tabs", "webNavigation"],
  "background": {
    "service_worker": "static/js/background.js" 
  },
  "action": {
    "default_popup": "index.html"   
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["static/js/contentScript.js"]  
    }
  ],
  "icons": {}
}
